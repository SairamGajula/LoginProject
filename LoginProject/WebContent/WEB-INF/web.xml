<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LoginProject</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>registerUser</servlet-name>
  <servlet-class>com.servlet.user.RegisterServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>registerUser</servlet-name>
  <url-pattern>/register</url-pattern>
  
  </servlet-mapping>
  
  <servlet>
  <servlet-name>allUsers</servlet-name>
  <servlet-class>com.servlet.user.GetUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>allUsers</servlet-name>
  <url-pattern>/allUsers</url-pattern>
  
  </servlet-mapping>
  
  
  
  
</web-app>